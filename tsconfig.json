      "@app/*": [
        "./src/app/@app/*"
      ],
      "@state/*": [
        "./src/app/@state/*"
      ],
      "@meta/*": [
        "./src/app/@meta/*"
      ],
      "@elementar/components/core": [
        "./projects/components/src/core"
      ],
      "@elementar/components/alert": [
        "./projects/components/src/alert"
      ],
